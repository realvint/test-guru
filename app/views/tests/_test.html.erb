<tr>
  <td><%= link_to test do %><%= test.title %><% end %></td>
  <td align="center"><%= test.level %></td>
  <td align="center"><%= test.questions.count %></td>
  <td><%= link_to('Редактировать', edit_test_path(test) )%></td>
  <td><%= link_to 'Удалить', test_path(test), method: :delete, data: { confirm: 'Удалить тест?' }  %></td>
  <td><%= button_to 'Start', start_test_path(test) %></td>
</tr>