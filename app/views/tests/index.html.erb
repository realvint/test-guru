<div class="card">
  <div class="card-header">
    <h1><%= t('.header') %></h1>
  </div>
  <div class="card-body">
    <table class="table">
      <thead>
      <tr>
        <th class="sort-by-title pointer">
          <%= t('.title') %>
          <%= octicon 'arrow-up', class: 'text-success hide' %>
          <%= octicon 'arrow-down', class: 'text-success hide' %>
        </th>
        <th><%= t('.level') %></th>
        <th><%= t('.questions') %></th>
        <th></th>
      </tr>
      </thead>
      <tbody>
      <%= render @tests %>
      </tbody>
    </table>
  </div>
</div>


