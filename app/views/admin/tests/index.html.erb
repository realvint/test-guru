<div class="card">
  <div class="card-header d-flex justify-content-between">
    <h1><%= t('.header') %></h1>
    <%= link_to t('.create'), new_admin_test_path, class: 'mt-3' %>
  </div>
  <div class="card-body">
    <%= render 'shared/errors', resource: @test %>
    <table class="table">
      <thead>
        <tr>
          <th class="sort-by-title pointer">
            <%= t('.title') %>
            <%= octicon 'arrow-up', class: 'text-success hide' %>
            <%= octicon 'arrow-down', class: 'text-success hide' %>
          </th>
          <th class="text-center"><%=t('.level') %></th>
          <th class="text-center"><%= t('.questions') %></th>
          <th class="text-center"><%= t('.timer') %></th>
          <th colspan="2"><%= t('.actions') %></th>
        </tr>
      </thead>
     <tbody>
      <%= render @tests %>
     </tbody>
    </table>
  </div>
</div>
